<Window x:Class="FactoryAutomation.Designer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FactoryAutomation.Designer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" DataContext="{Binding Path=Main, Source={StaticResource Locator}}">
    <StackPanel>
        <Button Width="50" Height="50" Command="{Binding ACommand}">A</Button>
        <Button Width="50" Height="50" Command="{Binding BCommand}">B</Button>
    </StackPanel>
</Window>
